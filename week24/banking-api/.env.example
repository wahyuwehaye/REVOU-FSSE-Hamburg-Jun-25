# Database
DATABASE_URL="mysql://root@localhost:3306/banking_api"

# JWT
JWT_SECRET="your-banking-secret-key-change-in-production"
JWT_REFRESH_SECRET="your-banking-refresh-secret-key-change-in-production"
JWT_EXPIRES_IN="15m"
JWT_REFRESH_EXPIRES_IN="7d"

# App
PORT=3000
NODE_ENV="development"

# CORS
CORS_ORIGIN="http://localhost:3000,http://localhost:3001"

# Transaction Limit
DAILY_TRANSFER_LIMIT=50000000
SINGLE_TRANSFER_LIMIT=10000000
